{
  "name": "fastube-monorepo",
  "version": "1.0.0",
  "description": "Youtube subtitles extractor with front and backend",
  "private": true,
  "workspaces": [
    "fastube-mono/frontend",
    "fastube-mono/backend"
  ],
  "scripts": {
    "start:frontend": "cd ./frontend && npm run dev",
    "start:backend": "cd ./backend && npm run start:dev",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "build:frontend": "cd fastube-mono/frontend && npm run build",
    "build:backend": "cd fastube-mono/backend && npm run build",
    "build": "npm run build:frontend && npm run build:backend",
    "install:all": "npm install && npm run install:frontend && npm run install:backend",
    "install:frontend": "cd fastube-mono/frontend && npm install",
    "install:backend": "cd fastube-mono/backend && npm install"
  },
  "keywords": [
    "youtube",
    "subtitles",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
